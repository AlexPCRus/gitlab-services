{
"name": "gitlab",
"desc": "Сервисы для работы с GitLab",
"enabled": true,
"groups": [
{
"name": "webhook",
"desc": "Обработка webhooks от GitLab.",
"template": "/webhook/{ТипВнешнегоХранилища}/{ИмяМетода}/*",
"methods": [
{
"name": "updates",
"desc": "Обновление данных в ИБ по изменениям в GitLab.",
"method": "POST"
}
]
},
{
"name": "version",
"desc": "Описание REST-сервиса.",
"template": "/*",
"methods": [
{
"name": "version",
"desc": "Описание REST-сервиса.",
"method": "GET"
}
]
}
]
}